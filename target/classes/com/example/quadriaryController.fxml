<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="601.0" prefWidth="463.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.QuadriaryController">
   <children>
      <Label fx:id="outputLabel" prefHeight="274.0" prefWidth="463.0">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Separator prefWidth="200.0" />
      <Label text="Inventory">
         <VBox.margin>
            <Insets bottom="20.0" left="10.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextItem" prefHeight="67.0" prefWidth="212.0" style="-fx-background-color: black;" text="Next" textAlignment="CENTER" textFill="#fffd00">
               <font>
                  <Font size="28.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="selectButton" mnemonicParsing="false" onAction="#displayItem" prefHeight="67.0" prefWidth="212.0" style="-fx-background-color: black;" text="Select" textAlignment="CENTER" textFill="#4eff00">
               <font>
                  <Font size="28.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="leaveButton" mnemonicParsing="false" onAction="#switchToPrimary" prefHeight="67.0" prefWidth="212.0" style="-fx-background-color: black; -fx-background-radius: 2;" text="Leave" textAlignment="CENTER" textFill="#5d93ff">
               <font>
                  <Font size="28.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
